{
  "name": "Pymakr",
  "displayName": "Pymakr",
  "description": "Adds a REPL console to the terminal that connects to your Pycom board. Run single files or synchronize your project files to your board.",
  "version": "0.1.5",
  "publisher": "pycom",
  "repository": "https://github.com/pycom/pymakr-vsc",
  "icon": "images/logo.png",
  "license": "SEE LICENSE IN <LICENSE.md>",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.11.0"
  },
  "keywords": [
    "pycom",
    "wipy",
    "lopy",
    "fipy",
    "iot"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./pymakr",
  "contributes": {
    "commands": [
      {
        "command": "pymakr.connect",
        "title": "Pymakr > Connect"
      },
      {
        "command": "pymakr.disconnect",
        "title": "Pymakr > Disconnect"
      },
      {
        "command": "pymakr.sync",
        "title": "Pymakr > Synchronize project"
      },
      {
        "command": "pymakr.run",
        "title": "Pymakr > Run current file"
      },
      {
        "command": "pymakr.projectSettings",
        "title": "Pymakr > Project Settings"
      },
      {
        "command": "pymakr.globalSettings",
        "title": "Pymakr > Global Settings"
      },
      {
        "command": "pymakr.extra.getVersion",
        "title": "Pymakr > Extra's > Get board version"
      },
      {
        "command": "pymakr.extra.getWifiMac",
        "title": "Pymakr > Extra's > Get WiFi AP SSID"
      },
      {
        "command": "pymakr.extra.getSerial",
        "title": "Pymakr > Extra's > List Serial Ports"
      },
      {
        "command": "pymakr.help",
        "title": "Pymakr > Help"
      },
      {
        "command": "pymakr.listCommands",
        "title": "Pymakr > List all Commands"
      }
    ],
    "keybindings": [
      {
        "command": "pymakr.connect",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      },
      {
        "command": "pymakr.sync",
        "key": "ctrl+shift+s",
        "mac": "cmd+shift+s"
      },
      {
        "command": "pymakr.run",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      },
      {
        "command": "pymakr.globalSettings",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "pymakr.sync",
          "group": "pymakr"
        },
        {
          "command": "pymakr.run",
          "group": "pymakr"
        }
      ],
      "editor/context": [
        {
          "command": "pymakr.connect",
          "group": "pymakr"
        },
        {
          "command": "pymakr.run",
          "group": "pymakr"
        },
        {
          "command": "pymakr.sync",
          "group": "pymakr"
        }
      ],
      "editor/title": [
        {
          "command": "pymakr.run",
          "group": "pymakr"
        },
        {
          "command": "pymakr.sync",
          "group": "pymakr"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "babel-watch": "babel src --out-dir lib",
    "install": "node install.js"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "electron-rebuild": "^1.6.0",
    "element-resize-detector": "1.1.11",
    "serialport": "^4.0.7",
    "telnet-client": "^0.13.0",
    "xterm": "^2.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "6.24.1",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vsce": "^1.35.0",
    "vscode": "^1.0.0"
  }
}
